@startuml

class Prenda {
    tipoDePrenda
    colorPrincipal
    colorSecundario
    material
}

class TipoDePrenda {
    tipo
    categoria
}

enum Tipo {
    ZAPATOS
    CAMISA_MANGA_CORTA
    PANTALON
}

enum Categoria {
    PARTE_SUPERIOR,
    CALZADO,
    PARTE_INFERIOR,
    ACCESORIOS
}

enum Material {
    CUERO
    JEAN
}

Prenda --> TipoDePrenda
Prenda --> Material

TipoDePrenda --> Tipo
TipoDePrenda --> Categoria

@enduml